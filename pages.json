{
  "name": "alux-design-system",
  "root": ".next/",
  "main": ".next/server/pages/index.js",
  "build": {
    "command": "npm run build",
    "output_directory": ".next"
  },
  "routes": [
    { "pattern": "/_next/static/*", "path": ".next/static" },
    { "pattern": "/_next/data/*", "path": ".next/data" },
    { "pattern": "/*", "path": ".next/server/pages/index.html" }
  ]
}
